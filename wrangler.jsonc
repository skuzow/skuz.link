{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "skuz-link",
  "compatibility_date": "2025-12-02",
  "main": "./.output/server/index.mjs",
  "assets": {
    "binding": "ASSETS",
    "directory": "./.output/public/"
  },
  "routes": [
    {
      "pattern": "skuz.link",
      "custom_domain": true
    }
  ],
  "preview_urls": true,
  "observability": {
    "enabled": true
  }
}
